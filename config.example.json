{
  "paths": {
    "workingDirectory": "C:\\path\\to\\powershell-console",
    "profilePath": "C:\\Users\\YourUsername\\Documents\\PowerShell\\Microsoft.PowerShell_profile.ps1",
    "oktaYamlPath": "C:\\Users\\YourUsername\\.okta\\okta.yaml",
    "rdcManagerPath": "C:\\path\\to\\RDCMan.exe",
    "linksPath": "C:\\Users\\YourUsername\\Favorites\\Links\\",
    "backupSource": "C:\\AppInstall\\",
    "backupDestination": "OneDrive - Company Name\\DevBackups",
    "devRoot": "C:\\path\\to\\dev",
    "vpnOutputPath": "C:\\path\\to\\vpn_output"
  },
  "defaultConnection": {
    "instance": "",
    "remoteIP": "",
    "localPort": "8443",
    "remotePort": "443"
  },
  "aws": {
    "defaultRegion": "us-east-1",
    "fortiGateDeviceTypeId": "7125681a"
  },
  "environments": {
    "manual": {
      "displayName": "Manual Account Selection",
      "region": "us-east-1",
      "highlightChar": "M",
      "highlightPos": 0,
      "defaultRemoteIP": "",
      "defaultRemotePort": "",
      "defaultLocalPort": "",
      "instances": {
        "jump-box": ""
      },
      "boxes": [],
      "actions": [
        "instanceManagement",
        "vpnConnections"
      ]
    },
    "exampleaccount": {
      "oktaProfileMap": {
        "Admin": "exampleaccount-OKTA-PROFILE-Admin",
        "devops": "exampleaccount-OKTA-PROFILE-devops"
      },
      "preferredRole": "Admin",
      "region": "us-east-1",
      "instances": {
        "jump-box": "",
        "remote-host": ""
      },
      "accountId": "123456789012",
      "defaultLocalPort": "8388",
      "defaultRemoteIP": "",
      "defaultRemotePort": "3389",
      "boxes": [],
      "actions": [
        "instanceManagement"
      ],
      "availableRoles": [
        "Admin",
        "devops"
      ],
      "displayName": "Example Account",
      "sessionDuration": "3600"
    }
  },
  "actionDefinitions": {
    "instanceManagement": {
      "text": "Instance Management",
      "highlightPos": 0,
      "highlightChar": "I",
      "function": "Show-InstanceManagementMenu"
    },
    "alohaBox": {
      "text": "Aloha to ETS-NETTOOLS Box",
      "highlightPos": 0,
      "highlightChar": "A",
      "function": "Show-AlohaBoxMenu"
    },
    "vpnConnections": {
      "text": "Get VPN Connections",
      "highlightPos": 0,
      "highlightChar": "G",
      "function": "Get-VpnConnections"
    }
  },
  "standardActions": [
    {
      "text": "Change AWS Account",
      "highlightPos": 7,
      "highlightChar": "C",
      "function": "Show-AwsAccountMenu"
    }
  ],
  "menus": {
    "Main Menu": [
      {
        "text": "Ping Google",
        "action": "\r\n            Start-InteractivePing -Target \"google.com\"\r\n            Invoke-StandardPause\r\n        "
      },
      {
        "text": "IP Config",
        "action": "\r\n            Show-NetworkConfiguration\r\n            Invoke-StandardPause\r\n        "
      },
      {
        "text": "AWS Login",
        "action": "\r\n            Start-AwsWorkflow\r\n        "
      },
      {
        "text": "PowerShell Profile Edit",
        "action": "\r\n            Invoke-Expression \"code '$($script:Config.paths.profilePath)'\"\r\n            Invoke-StandardPause\r\n        "
      },
      {
        "text": "Okta YAML Edit",
        "action": "\r\n            Invoke-Expression \"code '$($script:Config.paths.oktaYamlPath)'\"\r\n            Invoke-StandardPause\r\n        "
      },
      {
        "text": "Whitelist Links Folder",
        "action": "\r\n            Invoke-Expression \"icacls '$($script:Config.paths.linksPath)' /t /setintegritylevel m\"\r\n            Invoke-StandardPause\r\n        "
      },
      {
        "text": "Meraki Backup",
        "action": "\r\n            Start-MerakiBackup\r\n        "
      },
      {
        "text": "Code Count",
        "action": "\r\n            Start-CodeCount\r\n        "
      },
      {
        "text": "Backup Dev Environment",
        "action": "\r\n            Start-BackupDevEnvironment\r\n        "
      },
      {
        "text": "Package Manager",
        "action": "\r\n            Show-PackageManagerMenu\r\n        "
      }
    ],
    "Package Manager": [
      {
        "text": "Manage Updates",
        "action": "\r\n            Select-PackagesToUpdate\r\n            Invoke-StandardPause\r\n        "
      },
      {
        "text": "List Installed Packages",
        "action": "\r\n            Get-InstalledPackages\r\n            Invoke-StandardPause\r\n        "
      },
      {
        "text": "Search Packages",
        "action": "\r\n            Search-Packages\r\n            Invoke-StandardPause\r\n        "
      }
    ],
    "Instance Management": [
      {
        "text": "View Current Instance Settings",
        "action": " Show-CurrentInstanceSettings "
      },
      {
        "text": "List Running Instances",
        "action": " Get-RunningInstances "
      },
      {
        "text": "Set Default Instance ID",
        "action": " Set-DefaultInstanceId "
      },
      {
        "text": "Set Default Remote Host Info",
        "action": " Set-DefaultRemoteHostInfo "
      },
      {
        "text": "Aloha Remote Access",
        "action": " Start-AlohaRemoteAccess "
      },
      {
        "text": "Test Instance Connectivity",
        "action": " Test-InstanceConnectivity "
      },
      {
        "text": "Get VPN Connections",
        "action": " Get-VpnConnections "
      }
    ]
  },
  "awsAccountMenuOrder": [],
  "awsPromptIndicator": {
    "directoryMappings": {
      "C:\\path\\to\\project1": "123456789012",
      "C:\\path\\to\\project2": "987654321098"
    },
    "_comments": {
      "directoryMappings": "Map directories to their expected AWS account IDs (12 digits)",
      "usage": "This module is loaded via PowerShell profile, not console.ps1",
      "credentialsFile": "The module reads from your AWS credentials file (default: %USERPROFILE%\\.aws\\credentials)",
      "customPath": "If your AWS credentials file is in a non-standard location, you can modify the module to use a custom path",
      "oktaIntegration": "Works with okta-aws-cli which updates the [default] profile in credentials file",
      "setup": "See modules/aws-prompt-indicator/README.md for complete setup instructions and PowerShell profile integration"
    }
  },
  "lineCounter": {
    "globalExclusions": {
      "extensions": [
        ".avhdx",
        ".conf",
        ".csv",
        ".log",
        ".vhdx",
        ".vsix",
        ".zip"
      ],
      "pathPatterns": [
        "backup",
        "log",
        "npm-packages.json"
      ]
    },
    "projectExclusions": {
      "my-project": {
        "files": ["package-lock.json", "yarn.lock"],
        "pathPatterns": ["backup", "archive"]
      },
      "legacy-app": {
        "includeOnly": ["main.py", "config.py"]
      },
      "old-project": {
        "excludeAll": true
      },
      "api-project": {
        "pathPatterns": ["backups", "logs", "config"]
      },
      "data-project": {
        "extensions": [".csv", ".xlsx"],
        "filePatterns": ["temp_*", "cache_*"]
      }
    },
    "_comments": {
      "globalExclusions": "Applied to all projects",
      "extensions": "File extensions to exclude (e.g., '.log', '.vsix')",
      "pathPatterns": "Path segments to exclude (case-insensitive, matches if pattern appears anywhere in path)",
      "files": "Exact filenames to exclude",
      "filePatterns": "Filename patterns to exclude (supports wildcards like 'temp_*')",
      "includeOnly": "Whitelist specific files (all other files in project excluded)",
      "excludeAll": "Exclude entire project from counts"
    }
  },
  "configVersion": "config.11"
}
